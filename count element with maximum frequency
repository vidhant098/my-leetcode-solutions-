class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
         
           vector<  int  >freq(   101 , 0 ) ;
      int n = nums.size(); 

 
 int maxfrq = 0 ; 
      for(int i = 0 ; i<n  ; i++)
       {
         freq[nums[i]]++  ; 
         maxfrq = max(maxfrq ,freq[nums[i]]) ;             
   }   
      
     int count = 0 ; 
    for( int j  = 1 ;   j<101; j++)
    {
       if(freq[j] == maxfrq )
        count+=maxfrq ; 
    ) 
       return count ;   
    }
};
